{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "webmaesther/laravel-project-starter",
    "type": "project",
    "description": "My Laravel Starter Kit",
    "keywords": [
        "php",
        "laravel"
    ],
    "license": "MIT",
    "version": "1.0.0",
    "require": {
        "php": "^8.4",
        "ext-redis": "*",
        "filament/filament": "^4.0",
        "inertiajs/inertia-laravel": "^2.0.3",
        "laravel/cashier-paddle": "^2.6.2",
        "laravel/folio": "^1.1.10",
        "laravel/fortify": "^1.27",
        "laravel/framework": "^12.20.0",
        "laravel/horizon": "^5.33.1",
        "laravel/nightwatch": "^1.10.6",
        "laravel/octane": "^2.11",
        "laravel/pennant": "^1.17.1",
        "laravel/pulse": "^1.4.2",
        "laravel/reverb": "^1.5.1",
        "laravel/sanctum": "^4.1.2",
        "laravel/scout": "^10.16",
        "laravel/socialite": "^5.21",
        "laravel/tinker": "^2.10.1",
        "laravel/wayfinder": "^0.1.6",
        "typesense/typesense-php": "^5.1"
    },
    "require-dev": {
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-zip": "*",
        "lib-pcre": ">=7.0",
        "barryvdh/laravel-debugbar": "^3.15.4",
        "driftingly/rector-laravel": "^2.0.5",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.5",
        "laravel/envoy": "^2.10.2",
        "laravel/pail": "^1.2.3",
        "laravel/pint": "^1.24.0",
        "laravel/sail": "^1.43.1",
        "laravel/telescope": "^5.10.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "peckphp/peck": "^0.1.3",
        "pestphp/pest": "^3.8.2",
        "pestphp/pest-plugin-laravel": "^3.2",
        "pestphp/pest-plugin-livewire": "^3.0",
        "pestphp/pest-plugin-type-coverage": "^3.6.0",
        "pestphp/pest-plugin-watch": "^3.0",
        "rector/rector": "^2.1.1",
        "soloterm/solo": "^0.5"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": false
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi",
            "npm ci && npm run build",
            "npx husky init",
            "git init && git add . && git commit -m \"Install Starter Kit\""
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan solo"
        ],
        "test:deps": [
            "@composer install --ansi --no-interaction --prefer-dist --optimize-autoloader",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -r \"file_exists('database/testing.sqlite') || touch('database/testing.sqlite');\""
        ],
        "test:prep": [
            "@php artisan key:generate --ansi",
            "@php artisan migrate --ansi",
            "@php artisan optimize:clear --ansi"
        ],
        "test": [
            "@peck:check",
            "@lint:check",
            "@phpstan:check",
            "@pest:check"
        ],
        "pest:quick": [
            "pest --parallel"
        ],
        "pest:coverage": [
            "pest --parallel --coverage --min=100"
        ],
        "pest:mutate": [
            "pest --mutate --covered-only --parallel --min=100"
        ],
        "pest:types": [
            "pest --type-coverage --min=100"
        ],
        "pest:check": [
            "@pest:types",
            "@pest:coverage",
            "@pest:mutate"
        ],
        "rector:check": [
            "rector --dry-run"
        ],
        "pint:check": [
            "pint --test"
        ],
        "rector:format": [
            "rector"
        ],
        "pint:format": [
            "pint"
        ],
        "lint:check": [
            "@rector:check",
            "@pint:check"
        ],
        "lint:format": [
            "@rector:format",
            "@pint:format"
        ],
        "phpstan:check": [
            "phpstan"
        ],
        "peck:check": [
            "peck"
        ],
        "wayfinder:gen": [
            "@php artisan wayfinder:generate"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
